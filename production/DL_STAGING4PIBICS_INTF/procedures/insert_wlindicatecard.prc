CREATE OR REPLACE EDITIONABLE PROCEDURE "DL_STAGING4PIBICS_INTF"."INSERT_WLINDICATECARD" (p_date IN VARCHAR2) AUTHID DEFINER IS
PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN

     DL_STAGING4PIBICS.PKG_WATCHLIST.MERGE_WLINDICATECARD_YYYYMMDD(p_date);
     DL_STAGING4PIBICS_INTF.PKG_BL_BLACKLIST_CASES.INSERT_TARGET_WLINDICATECARD(p_date);

     commit;

END INSERT_WLINDICATECARD;
/
