CREATE OR REPLACE FORCE EDITIONABLE VIEW "PIBICSDM2"."VWOTNOTNON" ("DEPT_SEQNO", "ESTIMATEMONTH", "ESTIMATEYEAR", "PROFILE_SEQNO") AS 
  SELECT   A.DEPT_SEQNO,
            A.ESTIMATEMONTH,
            A.ESTIMATEYEAR,
            b.profile_seqno
     FROM   OT_NONAUTHORIZED a, OT_NONAUTHPERSON b
    WHERE   A.OTNONAUTHSEQNO = B.OTNONAUTHSEQNO ;
