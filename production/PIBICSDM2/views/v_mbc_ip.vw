  GRANT SELECT ON "PIBICSDM2"."V_MBC_IP" TO "BIOSUPPORT";
  GRANT DELETE ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT INSERT ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT SELECT ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT UPDATE ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT REFERENCES ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT READ ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT ON COMMIT REFRESH ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT QUERY REWRITE ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT DEBUG ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
  GRANT FLASHBACK ON "PIBICSDM2"."V_MBC_IP" TO "BIOSAADM";
