CREATE OR REPLACE EDITIONABLE TRIGGER "PIBICSDM2"."WLREMARK_TRG" 
BEFORE UPDATE OR INSERT ON PIBICSDM2.WLREMARK REFERENCING NEW AS NEW OLD AS OLD
FOR EACH ROW
DECLARE
BEGIN   
        update WATCHLIST set remark = :new.remk where wlcd=:new.wlcd;
END;
/
ALTER TRIGGER "PIBICSDM2"."WLREMARK_TRG" ENABLE;
