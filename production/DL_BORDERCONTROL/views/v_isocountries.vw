  GRANT UPDATE ON "DL_BORDERCONTROL"."V_ISOCOUNTRIES" TO "BIOSAADM";
  GRANT REFERENCES ON "DL_BORDERCONTROL"."V_ISOCOUNTRIES" TO "BIOSAADM";
