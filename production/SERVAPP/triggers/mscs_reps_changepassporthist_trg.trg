CREATE OR REPLACE EDITIONABLE TRIGGER "SERVAPP"."MSCS_REPS_CHANGEPASSPORTHIST_TRG" 
BEFORE INSERT ON MSCS_REPS_CHANGEPASSPORTHIST 
FOR EACH ROW 
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    IF INSERTING AND :NEW.PASSHIST_SEQNO IS NULL THEN
      SELECT MSCS_REPS_CHANGEPASSPORTHIST_SEQ.NEXTVAL INTO :NEW.PASSHIST_SEQNO FROM SYS.DUAL;
    END IF;
  END COLUMN_SEQUENCES;
END;
/
ALTER TRIGGER "SERVAPP"."MSCS_REPS_CHANGEPASSPORTHIST_TRG" ENABLE;
