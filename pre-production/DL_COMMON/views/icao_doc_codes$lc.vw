CREATE OR REPLACE FORCE EDITIONABLE VIEW "DL_COMMON"."ICAO_DOC_CODES$LC" ("KEY_VALUE", "LOCALE", "LOCALE$DLC", "DISPLAY_VALUE", "DISPLAY_VALUE$DLC", "DISPLAY_ORDER", "IS_ACTIVE", "INS_AT", "INS_BY", "DML_AT", "DML_BY", "DML_TYPE", "NOTICE", "NOTICE$DLC", "NUM_VALUE", "ROW_FLAG_MASK", "ROW_FLAG_MASK$I18N", "OFFICIAL_LANGUAGE", "COUNTRIES$RI", "COUNTRIES$I18N$RI", "ICAO", "COUNTRY") AS 
  select 
			 key_value
			,LOCALE
			,LOCALE$DLC
			,DISPLAY_VALUE
			,DISPLAY_VALUE$DLC
			,DISPLAY_ORDER
			,IS_ACTIVE
			,INS_AT
			,INS_BY
			,DML_AT as DML_AT
			,DML_BY
			,DML_TYPE
			,NOTICE
			,NOTICE$DLC
			,NUM_VALUE
			,ROW_FLAG_MASK
			,ROW_FLAG_MASK$I18N
			,OFFICIAL_LANGUAGE
			,COUNTRIES$RI
			,COUNTRIES$I18N$RI
			,icao
			,country
	FROM
		dl_common.countries$LC;
  GRANT SELECT ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO PUBLIC;
  GRANT SELECT ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO "BIO_BD";
  GRANT DELETE ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO "DERMALOG_PROXY";
  GRANT INSERT ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO "DERMALOG_PROXY";
  GRANT SELECT ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO "DERMALOG_PROXY";
  GRANT UPDATE ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO "DERMALOG_PROXY";
  GRANT SELECT ON "DL_COMMON"."ICAO_DOC_CODES$LC" TO "TESTADM";
